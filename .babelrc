{
  presets: [
    'flow',
  ],
  env: {
    test: {
      presets: [
        [
          'env',
          {
            targets: {
              node: 'current',
            },
          },
        ],
      ],
      plugins: [
        'transform-runtime',
        'istanbul',
        'lodash',
      ],
    },
    es5: {
      presets: [
        [
          'env',
          {
            forceAllTransforms: true,
          },
        ],
      ],
      plugins: [
        'transform-runtime',
        'lodash',
      ],
    },
  },
}